Blink Assignment
const int LED = 13;
void setup()
{
    pinMode(LED, OUTPUT);
}
void blinkLED (int timeon, int timeoff, int blinkcount)
{
    for (int i=0;i <blinkcount;i++)
    {
    digitalWrite(LED, HIGH);
    delay(timeon);
    digitalWrite(LED, LOW);
    delay(timeoff);
    }
}
void loop()
{
    blinkLED(1000,1000,10);
    blinkLED(100,100,50);
    blinkLED(1000,250,20);
}


